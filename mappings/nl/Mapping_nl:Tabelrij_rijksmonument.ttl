### Main TriplesMap
####################
<http://nl.dbpedia.org/resource/Mapping_nl:Tabelrij_rijksmonument>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/0> ,
			       <LongitudeMapping/0> ,
			       <SimplePropertyMapping/3> ,
			       <SimplePropertyMapping/2> ,
			       <SimplePropertyMapping/7> ,
			       <SimplePropertyMapping/5> ,
			       <SimplePropertyMapping/6> ,
			       <SimplePropertyMapping/4> ,
			       <ConstantMapping/1> ,
			       <ConditionalMapping/0/Condition/0/ClassMapping> ,
			       <LatitudeMapping/0> ,
			       <ConstantMapping/0> ,
			       <ConditionalMapping/0/Condition/0/SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/1> ,
			       <ConditionalMapping/0/Condition/1/SimplePropertyMapping/0> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:template  "http://en.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox:Tabelrij_rijksmonument" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "https://nl.wikipedia.org/wiki/{wikititle}" .


