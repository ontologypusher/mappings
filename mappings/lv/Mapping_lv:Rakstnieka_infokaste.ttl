### Main TriplesMap
####################
<http://lv.dbpedia.org/resource/Mapping_lv:Rakstnieka_infokaste>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/4> ,
			       <SimplePropertyMapping/2> ,
			       <StartDateMapping/0> ,
			       <SimplePropertyMapping/3> ,
			       <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/1> ,
			       <SimplePropertyMapping/5> ,
			       <EndDateMapping/0> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dbo:Writer ;
        rr:template  "http://lv.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox:Rakstnieka_infokaste" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "https://lv.wikipedia.org/wiki/{wikititle}" .


