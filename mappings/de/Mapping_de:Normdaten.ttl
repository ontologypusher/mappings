### Main TriplesMap
####################
<http://de.dbpedia.org/resource/Mapping_de:Normdaten>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/1> ,
			       <SimplePropertyMapping/2> ,
			       <SimplePropertyMapping/3> ,
			       <SimplePropertyMapping/4> ,
			       <SimplePropertyMapping/5> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     owl:Thing ;
        rr:template  "http://de.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox:Normdaten" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "https://de.wikipedia.org/wiki/{wikititle}" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<SimplePropertyMapping/5>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/5/OM> ;
        rr:predicate  dbo:ndlId .

### ObjectMap
<SimplePropertyMapping/5/OM>
        a              rr:ObjectMap ;
        rml:reference  "NDL" .


### Predicate Object Map
#########################
<SimplePropertyMapping/4>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/4/OM> ;
        rr:predicate  dbo:lccn .

### ObjectMap
<SimplePropertyMapping/4/OM>
        a              rr:ObjectMap ;
        rml:reference  "LCCN" .


### Predicate Object Map
#########################
<SimplePropertyMapping/3>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/3/OM> ;
        rr:predicate  dbo:viafId .

### ObjectMap
<SimplePropertyMapping/3/OM>
        a              rr:ObjectMap ;
        rml:reference  "VIAF" .


### Predicate Object Map
#########################
<SimplePropertyMapping/2>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/2/FTM> ;
        rr:predicate  owl:sameAs .

### Function Term Map
<SimplePropertyMapping/2/FTM>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <SimplePropertyMapping/2/FTM/FV> .

### Function Execution Mapping
<SimplePropertyMapping/2/FTM/FV>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <Function/SimplePropertyFunction> ,
			       <SimplePropertyMapping/2/FTM/FV/propertyParameterPOM> ,
			       <SimplePropertyMapping/2/FTM/FV/transformParameterPOM> ,
			       <SimplePropertyMapping/2/FTM/FV/prefixParameterPOM> ;
        rr:subjectMap          <SubjectMap/Function> .

### Parameters
<SimplePropertyMapping/2/FTM/FV/propertyParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/2/FTM/FV/propertyParameterPOM/OM> ;
        rr:predicate  dbf:propertyParameter .

<SimplePropertyMapping/2/FTM/FV/transformParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:object     "external" ;
        rr:predicate  dbf:transformParameter .

<SimplePropertyMapping/2/FTM/FV/prefixParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:object     "http://www.viaf.org/viaf/" ;
        rr:predicate  dbf:prefixParameter .

### References
<SimplePropertyMapping/2/FTM/FV/propertyParameterPOM/OM>
        a              rr:ObjectMap ;
        rml:reference  "VIAF" .


### Predicate Object Map
#########################
<SimplePropertyMapping/1>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/1/OM> ;
        rr:predicate  foaf:name .

### ObjectMap
<SimplePropertyMapping/1/OM>
        a              rr:ObjectMap ;
        rml:reference  "GNDName" ;
        rr:language    "de" .


### Predicate Object Map
#########################
<SimplePropertyMapping/0>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <SimplePropertyMapping/0/OM> ;
        rr:predicate  dbo:individualisedGnd .

### ObjectMap
<SimplePropertyMapping/0/OM>
        a              rr:ObjectMap ;
        rml:reference  "GND" .


######################
# Conditional Mappings
######################



############
# Functions
############


<Function/SimplePropertyFunction>
        a             rr:PredicateObjectMap ;
        rr:object     dbf:simplePropertyFunction ;
        rr:predicate  fno:executes .


### Functions SubjectMap
#########################

<SubjectMap/Function>
        a            rr:SubjectMap ;
        rr:class     fno:Execution ;
        rr:termType  rr:BlankNode .