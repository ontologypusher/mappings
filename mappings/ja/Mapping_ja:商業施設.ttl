### Main TriplesMap
####################
<http://ja.dbpedia.org/resource/Mapping_ja:商業施設>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <SimplePropertyMapping/0> ,
			       <SimplePropertyMapping/1> ,
			       <latitude> ,
			       <longitude> ;
        rr:subjectMap          <SubjectMap> .


### Main SubjectMap
####################
<SubjectMap>  a      rr:SubjectMap ;
        rr:class     dbo:Place ,
		     schema:ShoppingCenter ,
		     dbo:ArchitecturalStructure ,
		     owl:Thing ,
		     dbo:ShoppingMall ,
		     schema:Place ,
		     dbo:Building ,
		     dbo:Location ,
		     wikidata:Q41176 ,
		     wikidata:Q11315 ;
        rr:template  "http://ja.dbpedia.org/resource/{wikititle}" ;
        rr:termType  rr:IRI .


### LogicalSource
##################

<LogicalSource>  a                rml:LogicalSource ;
        rml:iterator              "Infobox" ;
        rml:referenceFormulation  ql:wikitext ;
        rml:source                "http://ja.wikipedia.org/wiki/Mapping_ja:商業施設?oldid=24482" .


###########
# Mappings
###########

### Predicate Object Map
#########################
<longitude>  a        rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap> ;
        rr:predicate  geo:long .

### Function Term Map
<longitude/ObjectMap>
        a                   fnml:FunctionTermMap ;
        fnml:functionValue  <longitude/ObjectMap/FV> .

### Function Execution Mapping
<longitude/ObjectMap/FV>
        a                      rr:TriplesMap ;
        rml:logicalSource      <LogicalSource> ;
        rr:predicateObjectMap  <Function/LongitudeFunction> ,
			       <longitude/ObjectMap/FV/lonDegreesParameterPOM> ,
			       <longitude/ObjectMap/FV/lonMinutesParameterPOM> ,
			       <longitude/ObjectMap/FV/lonSecondsParameterPOM> ,
			       <longitude/ObjectMap/FV/lonDirectionParameterPOM> ;
        rr:subjectMap          <SubjectMap/Function> .

### Parameters
<longitude/ObjectMap/FV/lonDegreesParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap/FV/lonDegreesParameterPOM/ObjectMap> ;
        rr:predicate  dbf:lonDegreesParameter .

<longitude/ObjectMap/FV/lonSecondsParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap/FV/lonSecondsParameterPOM/ObjectMap> ;
        rr:predicate  dbf:lonSecondsParameter .

<longitude/ObjectMap/FV/lonMinutesParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap/FV/lonMinutesParameterPOM/ObjectMap> ;
        rr:predicate  dbf:lonMinutesParameter .

<longitude/ObjectMap/FV/lonDirectionParameterPOM>
        a             rr:PredicateObjectMap ;
        rr:objectMap  <longitude/ObjectMap/FV/lonDirectionParameterPOM/ObjectMap> ;
        rr:predicate  dbf:lonDirectionParameter .

### References
<longitude/ObjectMap/FV/lonDirectionParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "null" .

<longitude/ObjectMap/FV/lonSecondsParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "経度秒" .

<longitude/ObjectMap/FV/lonDegreesParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "経度度" .

<longitude/ObjectMap/FV/lonMinutesParameterPOM/ObjectMap>
        a              rr:ObjectMap ;
        rml:reference  "経度分" .


